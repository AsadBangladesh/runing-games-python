FloatLayout:
    Background:
        id: background
        canvas.before:
            Rectangle:
                size: self.size
                pos: self.pos
                source: "sky.png"
            Rectangle:
                size: self.width, 77
                pos: self.pos[0], self.pos[1] + self.height - 77
                texture: self.cloud_texture
            Rectangle:
                size: self.width, 100
                pos: self.pos[0], self.pos[1] 
                texture: self.floor_texture



